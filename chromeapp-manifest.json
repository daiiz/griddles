{
  "name": "MyChromeApp",
  "description": "my chrome app",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "version": "0.0.1",
  "icons": {
    "48": "icon.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "offline_enabled": true,
  "permissions": [
    "<all_urls>", 
    "storage"
  ]
}
